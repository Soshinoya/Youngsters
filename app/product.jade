doctype html
html(lang='ru')
    head
        meta(charset="UTF-8")
        meta(http-equiv="X-UA-Compatible" content="ie=edge")
        meta(name="viewport" content="width=device-width, initial-scale=1.0")
        link(href='./assets/apple-touch-icon.png' rel='apple-touch-icon' sizes='180x180')
        link(type='image/png' href='./assets/favicon-32x32.png' rel='icon' sizes='32x32')
        link(type='image/png' href='./assets/favicon-16x16.png' rel='icon' sizes='16x16')
        link(href='./assets/site.webmanifest' rel='manifest')
        title Youngsters - Кольцо Green Stone
        link(href="css/main.min.css" rel="stylesheet")
        script(src="js/minified/main.min.js" defer)
        script(src="js/minified/product.min.js" defer)
    body
        include ./components/templates/sidebar.jade

        include ./components/organisms/header.jade
        include ./components/organisms/footer.jade
        include ./components/organisms/headerSidebar.jade
        include ./components/organisms/sidebarCompositions/sidebarProductCharacteristics.jade
        include ./components/organisms/sidebarCompositions/sidebarProductSize.jade
        include ./components/organisms/sidebarCompositions/sidebarProductMenu.jade
        include ./components/organisms/sidebarCompositions/sidebarProductWarranty.jade
        include ./components/organisms/sidebarCompositions/sidebarProductCare.jade

        include ./components/molecules/categoriesHeader.jade
        include ./components/molecules/productImagesSlider.jade
        include ./components/molecules/card.jade

        include ./components/atoms/buttons/buttonFirst.jade
        include ./components/atoms/breadcrumbs.jade
        include ./components/atoms/price.jade
        include ./components/atoms/colorPick.jade
        include ./components/atoms/counter.jade
        include ./components/atoms/likeButton.jade

        +header('black')
        +headerSidebar()
        main.main
            .categories-header__wrapper
                +categoriesHeader()
                - var breadcrumbsPages = [{title: 'Главная',href: '#'},{title: 'Каталог',href: '#'},{title: 'Кольца',href: '#'},{title: 'Серебряное массивное кольцо Green Stone ',href: '#'}]
                .container
                    +breadcrumbs(breadcrumbsPages)
            .product
                .container
                    .product__inner
                        +productImagesSlider([{src: './images/content/products/rings/golden-stone/1.webp',srcset: './images/content/products/rings/golden-stone/1.webp 1x, ./images/content/products/rings/golden-stone/1@2x.webp 2x', alt: 'Кольцо Green Stone'},{src: './images/content/products/rings/golden-stone/2.webp',srcset: './images/content/products/rings/golden-stone/2.webp 1x, ./images/content/products/rings/golden-stone/2@2x.webp 2x', alt: 'Кольцо Green Stone'},{src: './images/content/products/rings/golden-stone/3.webp',srcset: './images/content/products/rings/golden-stone/3.webp 1x, ./images/content/products/rings/golden-stone/3@2x.webp 2x', alt: 'Кольцо Green Stone'}])
                        .product-info
                            .product-info-top
                                p.product-info-top__subtitle Артикул 123456
                                .product-info-top__inner
                                    h1.product-info-top__title Серебряное массивное кольцо 
                                        span green Stone
                                    +price('35 000,00')
                            .product-info__holder
                                ul.product-info__features
                                    li.product-info__features-item Серебро 925 проба
                                    li.product-info__features-item Средний вес 9,30 гр
                                    li.product-info__features-item Вставка - Оникс 28 ct
                                .product-info-config
                                    .product-info-config__item.product-info-config__item
                                        p.product-info-config__item-title Цвет: Позолоченное серебро
                                        .product-info-config__item-colors
                                            +colorPick('#E9D590', 'beige', 'product-color', 'active')
                                            +colorPick('#C8C8C8', 'gray', 'product-color')
                                            +colorPick('#141318', 'black', 'product-color')
                                    .product-info-config__item.product-info-config__item--size
                                        p.product-info-config__item-title Размер
                                        .product-info-config__item-size(data-sidebar-target="product-size")
                                            .product-info-config__item-text__wrapper
                                                p.product-info-config__item-text 29,5
                                            svg.product-info-config__item-icon
                                                use(xlink:href="./images/sprite.svg#triangle")
                                        +sidebarProductSize()
                                    .product-info-config__item.product-info-config__item--count
                                        p.product-info-config__item-title Количество
                                        +counter(1, 1, 5, 'product-count')
                                .product-info-actions
                                    .product-info-actions__btns
                                        +buttonFirst('Добавить в корзину')
                                        +likeButton()
                                    a.product-info-actions__item(href="#")
                                        svg.product-info-actions__item-icon
                                            use(xlink:href="./images/sprite.svg#whatsapp")
                                        .product-info-actions__item-text Начать чат со стилистом в WhatsApp
                                    a.product-info-actions__item(href="#")
                                        svg.product-info-actions__item-icon
                                            use(xlink:href="./images/sprite.svg#delivery")
                                        .product-info-actions__item-text Доставка по РФ 2 - 3 дня
                            .product-info-benefits
                                .product-info-benefits__col
                                    .product-info-benefits__item
                                        .product-info-benefits__item-inner(data-sidebar-target="product-characteristics")
                                            svg.product-info-benefits__icon
                                                use(xlink:href="./images/sprite.svg#plus-math")
                                            p.product-info-benefits__title Характеристики изделия
                                        +sidebarProductCharacteristics()
                                    .product-info-benefits__item
                                        .product-info-benefits__item-inner(data-sidebar-target="product-menu")
                                            svg.product-info-benefits__icon
                                                use(xlink:href="./images/sprite.svg#plus-math")
                                            p.product-info-benefits__title Доставка и оплата
                                        +sidebarProductMenu()
                                .product-info-benefits__col
                                    .product-info-benefits__item
                                        .product-info-benefits__item-inner(data-sidebar-target="product-warranty")
                                            svg.product-info-benefits__icon
                                                use(xlink:href="./images/sprite.svg#plus-math")
                                            p.product-info-benefits__title Гарантия и возврат
                                        +sidebarProductWarranty()
                                    .product-info-benefits__item
                                        .product-info-benefits__item-inner(data-sidebar-target="product-care")
                                            svg.product-info-benefits__icon
                                                use(xlink:href="./images/sprite.svg#plus-math")
                                            p.product-info-benefits__title Уход за изделием
                                        +sidebarProductCare()
            .goods-slider
                .container
                    .goods-slider__inner
                        .goods-slider__header
                            h2.goods-slider__title Вам может понравиться
                            .goods-slider__nav
                                svg.swiper-button-prev
                                    use(xlink:href="./images/sprite.svg#right")
                                .goods-slider-pagination--md.swiper-pagination
                                svg.swiper-button-next
                                    use(xlink:href="./images/sprite.svg#right")
                        .goods-slider__cards
                            .swiper
                                .swiper-wrapper
                                    - var cardsData = [{imgSrc:"./images/content/products/mas-ring.png",imgAlt:"Кольцо",title:"Кольцо YOUNGSTERS",price:"35 000,00",href:"/catalog.html",isFavorite:!1,type:"ring",isPlaque:!0,plaqueTopText:"Бестселлер",plaqueBottomText:"-10%"},{imgSrc:"./images/content/products/bracelet.png",imgAlt:"Браслет",title:"Браслет YOUNGSTERS",price:"35 000,00",href:"/catalog.html",isFavorite:!1,type:"bracelet",isPlaque:!0,plaqueTopText:"Бестселлер",plaqueBottomText:"10%"},{imgSrc:"./images/content/products/necklace.png",imgAlt:"Колье",title:"Колье YOUNGSTERS",price:"35 000,00",href:"/catalog.html",isFavorite:!1,type:"necklace",isPlaque:!0,plaqueTopText:"Бестселлер",plaqueBottomText:"10%"},{imgSrc:"./images/content/products/cuff.png",imgAlt:"Кафф",title:"Кафф YOUNGSTERS",price:"35 000,00",href:"/catalog.html",isFavorite:!1,type:"cuff",isPlaque:!0,plaqueTopText:"Бестселлер",plaqueBottomText:"10%"},{imgSrc:"./images/content/products/earrings.png",imgAlt:"Серьги",title:"Серьги YOUNGSTERS",price:"35 000,00",href:"/catalog.html",isFavorite:!1,type:"earrings",isPlaque:!0,plaqueTopText:"Бестселлер",plaqueBottomText:"10%"},{imgSrc:"./images/content/products/mas-ring.png",imgAlt:"Кольцо",title:"Кольцо YOUNGSTERS",price:"35 000,00",href:"/catalog.html",isFavorite:!1,type:"ring",isPlaque:!0,plaqueTopText:"Бестселлер",plaqueBottomText:"-10%"},{imgSrc:"./images/content/products/bracelet.png",imgAlt:"Браслет",title:"Браслет YOUNGSTERS",price:"35 000,00",href:"/catalog.html",isFavorite:!1,type:"bracelet",isPlaque:!0,plaqueTopText:"Бестселлер",plaqueBottomText:"10%"},{imgSrc:"./images/content/products/necklace.png",imgAlt:"Колье",title:"Колье YOUNGSTERS",price:"35 000,00",href:"/catalog.html",isFavorite:!1,type:"necklace",isPlaque:!0,plaqueTopText:"Бестселлер",plaqueBottomText:"10%"},{imgSrc:"./images/content/products/cuff.png",imgAlt:"Кафф",title:"Кафф YOUNGSTERS",price:"35 000,00",href:"/catalog.html",isFavorite:!1,type:"cuff",isPlaque:!0,plaqueTopText:"Бестселлер",plaqueBottomText:"10%"},{imgSrc:"./images/content/products/earrings.png",imgAlt:"Серьги",title:"Серьги YOUNGSTERS",price:"35 000,00",href:"/catalog.html",isFavorite:!1,type:"earrings",isPlaque:!0,plaqueTopText:"Бестселлер",plaqueBottomText:"10%"},{imgSrc:"./images/content/products/earrings.png",imgAlt:"Серьги",title:"Серьги YOUNGSTERS",price:"35 000,00",href:"/catalog.html",isFavorite:!1,type:"earrings",isPlaque:!0,plaqueTopText:"Бестселлер",plaqueBottomText:"10%"},{imgSrc:"./images/content/products/earrings.png",imgAlt:"Серьги",title:"Серьги YOUNGSTERS",price:"35 000,00",href:"/catalog.html",isFavorite:!1,type:"earrings",isPlaque:!0,plaqueTopText:"Бестселлер",plaqueBottomText:"10%"}];
                                    for cardInfo in cardsData
                                        .swiper-slide
                                            .goods-slider-slide
                                                +card(cardInfo)
                        .goods-slider-pagination--sm.swiper-pagination
            .product-social
                .container
                    .product-social__inner
                        h2.product-social__title
                            span @
                            | YOUNGSTERS
                        .product-social__items
                            .product-social__img
                                img(src="./images/content/banners/catalog/1.jpg", alt="Мужчина позирует в одежде Youngsters")
                            .product-social__info
                                p.product-social__info-text Наши подписчики выбирают самые классные украшения
                                .product-social__info-icons
                                    a.product-social__info-icon(href="/")
                                        svg
                                            use(xlink:href="./images/sprite.svg#facebook-black")
                                    a.product-social__info-icon(href="/")
                                        svg
                                            use(xlink:href="./images/sprite.svg#instagram-black")
                                    a.product-social__info-icon(href="/")
                                        svg
                                            use(xlink:href="./images/sprite.svg#vk-black")
                                    a.product-social__info-icon(href="/")
                                        svg
                                            use(xlink:href="./images/sprite.svg#telegram-black")
                            .product-social__img
                                img(src="./images/content/banners/catalog/3.jpg", alt="Украшения Youngsters")
                            .product-social__img.product-social__img--sm
                                img(src="./images/content/banners/catalog/3.jpg", alt="Украшения Youngsters")
                            .product-social__img
                                img(src="./images/content/banners/catalog/2.jpg", alt="Мужчина позирует в одежде Youngsters")
        +footer()


    //- svg
    //- use(xlink:href="./images/sprite.svg")
