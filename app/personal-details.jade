doctype html
html(lang='ru')
    head
        meta(charset="UTF-8")
        meta(http-equiv="X-UA-Compatible" content="ie=edge")
        meta(name="viewport" content="width=device-width, initial-scale=1.0")
        link(href='./assets/apple-touch-icon.png' rel='apple-touch-icon' sizes='180x180')
        link(type='image/png' href='./assets/favicon-32x32.png' rel='icon' sizes='32x32')
        link(type='image/png' href='./assets/favicon-16x16.png' rel='icon' sizes='16x16')
        link(href='./assets/site.webmanifest' rel='manifest')
        title Youngsters - Мои данные
        link(href="css/main.min.css" rel="stylesheet")
        script(src="js/minified/main.min.js" defer)
        script(src="js/minified/personal-details.min.js" defer)
    body
        include ./components/organisms/header.jade
        include ./components/organisms/footer.jade
        include ./components/organisms/headerSidebar.jade

        include ./components/molecules/categoriesHeader.jade
        include ./components/molecules/personalNav.jade

        include ./components/atoms/breadcrumbs.jade

        +header('black')
        +headerSidebar()
        main.main
            .categories-header__wrapper
                +categoriesHeader()
                - var breadcrumbsPages = [{title: 'Главная',href: '#'},{title: 'Мои данные',href: '#'}]
                .container
                    +breadcrumbs(breadcrumbsPages)
            .main__inner--personal
                section.personal-details
                    .container
                        .personal-container.personal-details__holder
                            +personalNav(1)
                            .personal-details__inner
                                .personal-details-main
                                    .personal-section-header
                                        p.personal-section-header__title Мои данные
                                        a.personal-section-header__link(href="edit-details.html")
                                            svg.personal-section-header__icon
                                                use(xlink:href="./images/sprite.svg#edit")
                                            p.personal-section-header__text Редактировать
                                    .personal-section__fields
                                        .personal-section-field
                                            p.personal-section-field__title Имя
                                            p.personal-section-field__text Иван
                                        .personal-section-field
                                            p.personal-section-field__title Фамилия
                                            p.personal-section-field__text Иванов
                                        .personal-section-field
                                            p.personal-section-field__title E-mail
                                            p.personal-section-field__text ivan@gmail.com
                                        .personal-section-field
                                            p.personal-section-field__title Телефон
                                            p.personal-section-field__text + 7 000 000 00 00
                                .personal-details__img
                                    img(src="./images/content/banners/personal/3.jpg" alt="Мужчина позирует в украшениях Youngsters")
                            .personal-details-address
                                .personal-section-header
                                    p.personal-section-header__title Мои Адреса
                                    a.personal-section-header__link(href="add-address.html")
                                        svg.personal-section-header__icon
                                            use(xlink:href="./images/sprite.svg#plus-math")
                                        p.personal-section-header__text Добавить
                                .personal-details-address-slider
                                    .swiper
                                        .swiper-wrapper
                                            .swiper-slide
                                                .personal-details-address-item
                                                    p.personal-details-address-item__title Иван Иванов
                                                    p.personal-details-address-item__subtitle Адрес доставки по умолчанию
                                                    p.personal-details-address-item__text г. Москва, м. Текстильщики, 8-я улица Текстильщиков, д. 10
                                                    p.personal-details-address-item__text + 7 000 000 00 00
                                            .swiper-slide
                                                .personal-details-address-item
                                                    p.personal-details-address-item__title Иван Иванов
                                                    p.personal-details-address-item__subtitle Адрес доставки по умолчанию
                                                    p.personal-details-address-item__text г. Москва, м. Текстильщики, 8-я улица Текстильщиков, д. 10
                                                    p.personal-details-address-item__text + 7 000 000 00 00
                                            .swiper-slide
                                                .personal-details-address-item
                                                    p.personal-details-address-item__title Иван Иванов
                                                    p.personal-details-address-item__subtitle Адрес доставки по умолчанию
                                                    p.personal-details-address-item__text г. Москва, м. Текстильщики, 8-я улица Текстильщиков, д. 10
                                                    p.personal-details-address-item__text + 7 000 000 00 00
                                            .swiper-slide
                                                .personal-details-address-item
                                                    p.personal-details-address-item__title Иван Иванов
                                                    p.personal-details-address-item__subtitle Адрес доставки по умолчанию
                                                    p.personal-details-address-item__text г. Москва, м. Текстильщики, 8-я улица Текстильщиков, д. 10
                                                    p.personal-details-address-item__text + 7 000 000 00 00
                                            .swiper-slide
                                                .personal-details-address-item
                                                    p.personal-details-address-item__title Иван Иванов
                                                    p.personal-details-address-item__subtitle Адрес доставки по умолчанию
                                                    p.personal-details-address-item__text г. Москва, м. Текстильщики, 8-я улица Текстильщиков, д. 10
                                                    p.personal-details-address-item__text + 7 000 000 00 00
                                .personal-details-address-slider-pagination--sm.swiper-pagination
                                .personal-details-address-slider__nav
                                    svg.swiper-button-prev
                                        use(xlink:href="./images/sprite.svg#right")
                                    .personal-details-address-slider-pagination--md.swiper-pagination
                                    svg.swiper-button-next
                                        use(xlink:href="./images/sprite.svg#right")
        +footer()


    //- svg
    //- use(xlink:href="./images/sprite.svg")